{
  "name": "dsplay-template-react-boilerplate",
  "version": "1.1.0",
  "private": true,
  "dependencies": {
    "@dsplay/react-template-utils": "^2.0.0",
    "node-sass": "^4.14.1",
    "react": "^16.13.1",
    "react-app-polyfill": "^1.0.6",
    "react-dom": "^16.13.1",
    "react-scripts": "^3.4.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "prebuild": "npm run linter",
